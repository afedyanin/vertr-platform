{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information",
      "System.Net.Http.HttpClient": "Warning"
    },
    "Console": {
      "TimestampFormat": "[yyyy-MM-dd HH:mm:ss] "
    }
  },
  "ConnectionStrings": {
    "RedisConnection": "localhost"
  },
  "TinvestGateway": {
    "BaseAddress": "http://localhost:8085"
  },
  "VertrForecastGateway": {
    "BaseAddress": "http://localhost:8081"
  },
  "SubscriptionSettings": {
    "Portfolios": {
      "Channel": "portfolios",
      "IsEnabled": true
    },
    "OrderBooks": {
      "Channel": "market.orderBooks",
      "IsEnabled": true
    },
    "Candles": {
      "Channel": "market.candles.*",
      "IsEnabled": true
    }
  },
  "Predictors": [
    "naive",
    "random_walk"
  ]
}